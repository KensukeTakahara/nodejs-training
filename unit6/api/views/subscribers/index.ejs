<h2>Subscriber Table</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>ZipCode</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tody>
    <% subscribers.forEach(subscriber => { %>
    <tr>
      <td>
        <a href="<%= `/subscribers/${subscriber._id}` %>">
          <%= subscriber.name %>
        </a>
      </td>
      <td>
        <%= subscriber.email %>
      </td>
      <td>
        <%= subscriber.zipCode %>
      </td>
      <td>
        <a href="<%= `/subscribers/${subscriber._id}/edit` %>">
          Edit
        </a>
      </td>
      <td>
        <a
          href="<%= `/subscribers/${subscriber._id}/delete?_method=DELETE` %>"
          onclick="return confirm('Are you sure want to delete this record?')"
        >
          Delete
        </a>
      </td>
    </tr>
    <% }); %>
  </tody>
</table>
